<body>
  <main>
    <div class="container">
      <div class="row">
        <div class="mx-auto col-11">
          <h5 class="genres-index">ジャンル一編集</h5>
          <%= form_with model: @genre, url:admin_genre_path(@genre.id), local:true do |f| %>
          <h6>ジャンル名</h6>
          <%= f.text_field :name %>

          <%= f.submit '変更を保存', class: 'btn btn-success' %>

          <% end %>
        </div>
      </div>
    </div>
  </main>
</body>
